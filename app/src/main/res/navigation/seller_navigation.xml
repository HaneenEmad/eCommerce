<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/seller_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.ecommerce.Sellers.ui.SellerHomeFragment"
        android:label="fragment_seller_home"
        tools:layout="@layout/fragment_seller_home" >
        <action
            android:id="@+id/action_sellerHomeFragment_to_sellerAddFragment"
            app:destination="@+id/navigation_add" />
    </fragment>
    <fragment
        android:id="@+id/navigation_add"
        android:name="com.example.ecommerce.Sellers.SellerProductCategoryFragment"
        android:label="fragment_seller_add"
        tools:layout="@layout/fragment_seller_product_category" />
</navigation>